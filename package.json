	},
	"dependencies": {
		"@supabase/supabase-js": "^2.45.2",
		"vue": "^3.4.38",
		"vue-router": "^4.4.3"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.1.3",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vue-tsc": "^2.1.2"
	}
}
