{
	"name": "blood-management-system",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite --open",
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
